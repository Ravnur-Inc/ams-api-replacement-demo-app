# Media Services credentials
AZURE_SUBSCRIPTION_ID=your-subscription-id
AZURE_RESOURCE_GROUP_NAME=your-resource-group-name
RAVNUR_MEDIA_SERVICES_ACCOUNT_NAME=default
RAVNUR_API_ENDPOINT=https://your-ravnur-api-endpoint/
RAVNUR_API_KEY=<API KEY>

# Event Grid configuration (required when USE_EVENT_GRID=true)
EVENT_GRID_ENDPOINT=your-event-grid-topic-endpoint
EVENT_GRID_KEY=your-event-grid-access-key

# Service Bus configuration (required when USE_EVENT_GRID=false)
SERVICE_BUS_NAMESPACE=your-service-bus-namespace.servicebus.windows.net
SERVICE_BUS_QUEUE_NAME=your-queue-name

# Event Grid configuration (required when USE_EVENT_GRID=true)
USE_EVENT_GRID=false
EVENT_GRID_ENDPOINT=your-event-grid-topic-endpoint
EVENT_GRID_KEY=your-event-grid-access-key

